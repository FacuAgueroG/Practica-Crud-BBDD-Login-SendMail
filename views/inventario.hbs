<body class="bodyinventario">
    

<div class="container mt-5">
<table class="table table-dark table-striped mt-5">
  <thead>
      <tr class="text-center">
          <th colspan="8">
          <a href="/addItem"><button class="btn btn-primary">Agregar</button></a>
      
      </th></tr>
    <tr>
      <th class="text-center" scope="col">ID</th>
      <th scope="col">Modelo</th>
      <th class="text-center" scope="col">Tipo</th>
      <th scope="col">Descripcion</th>
      <th class="text-center" scope="col">Cantidad</th>
      <th class="text-center" scope="col">Imagen</th>
      <th scope="col" ></th>
    </tr>
  </thead>
  <tbody>
      {{#each data}}
    <tr>
      <td class="text-center">{{id}}</td>
      <td>{{modelo}}</td>
      <td class="text-uppercase text-center">{{tipo}}</td>
      <td>{{descripcion}}</td>
      <td class="text-center">{{cantidad}}</td>
      <td class="text-center"><img src={{imageURL}} alt={{modelo}}></td>
      <td class="text-center">
      <a href="/handleedit/{{id}}"><button class="btn btn-danger">Borrar</button></a>
      <a href="/handleedit/{{id}}"><button class="btn btn-warning">Cambiar</button></a></td>
    </tr>
    {{/each}}
    
  </tbody>
</table>
</div>
</body>